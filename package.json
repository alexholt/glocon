{
  "name": "GloCon",
  "version": "1.0.0",
  "description": "Global Conflict Simulator",
  "main": "src/server/index.js",
  "scripts": {
    "start-client": "SERVER_URL=http://localhost:8000 NODE_ENV=development webpack-dev-server --hot --config webpack.config.js --colors --progress",
    "build": "NODE_ENV=production webpack --config webpack.config.js -p",
    "start-server": "CLIENT_URL=http://localhost:8080 NODE_ENV=development DB_URL=mongodb://localhost:27017/chatcafe nodemon src/server/index.js",
    "start-mongo": "mongod --dbpath=/data",
    "seed-mongo": "node src/server/seed.js",
    "run-prod": "NODE_ENV=production DB_URL=mongodb://localhost:27017/chatcafe nodemon src/server/index.js",
    "redirect-ports": "iptables -t nat -A PREROUTING -i eth0 -p tcp --dport 80 -j REDIRECT --to-port 8000",
    "test": "DB_URL=mongodb://localhost:27017/chatcafe mocha --compilers js:babel-core/register --reporter spec",
    "test-watch": "DB_URL=mongodb://localhost:27017/chatcafe mocha --compilers js:babel-core/register --reporter spec --watch",
    "test-break": "DB_URL=mongodb://localhost:27017/chatcafe mocha --compilers js:babel-core/register --debug-brk"
  },
  "repository": {
    "type": "git",
    "url": "https://www.github.com/alexholt/glocon"
  },
  "keywords": [
    "game"
  ],
  "author": "Alex Holt",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "connect-mongo": "^1.3.2",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "lodash": "^4.16.4",
    "mocha": "^3.0.2",
    "mongodb": "^2.2.10",
    "mongoose": "^4.6.1",
    "node-sass": "^3.10.0",
    "normalize.css": "^4.2.0",
    "postcss": "^5.2.0",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "sass-loader": "^4.0.2",
    "stats.js": "^0.17.0",
    "style-loader": "^0.13.1",
    "svg-inline-loader": "^0.7.0",
    "svg-loader": "0.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "nodemon": "^1.10.2"
  }
}
